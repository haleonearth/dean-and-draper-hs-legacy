          "starting_html" : "",
          "value" : "<!-- Color Styles -->\n{% color \"select_body_color\" label='Choose Body Text Color', color='#000000', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_H1_title_color\" label='Choose H1 Title Color', color='#ffffff', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_accent_color\" label='Choose Template Accent Color', color='#2cb6b6', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_link_color\" label='Choose Template Link Color', color='#2cb6b6', no_wrapper=True, export_to_template_context=True %}\n\n<!-- Header and Footer -->\n{% choice \"header_option\" label='Add or Remove Header', value='On', choices='On, Off' export_to_template_context=True %}\n{% color \"select_header_bgcolor\" label='Choose Header Background Color', color='#ffffff', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_footer_bgcolor\" label='Choose Footer Background Color', color='#eeeeee', no_wrapper=True, export_to_template_context=True %}\n<!-- Container Sidebar Option -->\n{% choice \"container_bg_side\" label='Add or Remove Background Container Around Sidebar Content (Solid and See-Through)', value='On (Transparent Container)', choices='Off, On (Solid Container), On (Transparent Container)' export_to_template_context=True %}\n{% color \"container_bg_side_color\" label='Choose Content Background Sidebar Container Color<br><hr style=\"height:5px; display:block; margin-top:14px; color:#2cb6b6; background-color:#2cb6b6;\">', color='#EEEEEE', no_wrapper=True, export_to_template_context=True %}\n\n<!-- Form Options -->\n{% color \"select_form_titlecolor\" label='Choose Form Title Color', color='#000000', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_form_fieldbgcolor\" label='Choose Form Field Background Color', color='#ffffff', no_wrapper=True, export_to_template_context=True %}\n{% color \"select_form_fieldcolor\" label='Choose Form Field Text Color<br><hr style=\"height:5px; display:block; margin-top:14px; color:#2cb6b6; background-color:#2cb6b6;\">', color='#444444', no_wrapper=True, export_to_template_context=True %}\n\n\n<!-- HUBL STYLE OPTIONS -->\n<style>\n/* Main Body Styles */\nbody.free {\n    background-color:{{ widget_data.body_free_bg_color.color }};\n    background-image:url('{{ widget_data.body_free_bg_img.src }}');\n}\nbody.free, body.free h3, body.free p, body.free ul li, body.free ol li {color:{{ widget_data.select_body_color.color }};} \nbody.free h1 {color:{{ widget_data.select_H1_title_color.color }}!important;}\nbody.free h2, body.free h4 {color:{{ widget_data.select_accent_color.color }}!important;}\nbody.free a, body.free a:link, body.free a:active {color:{{ widget_data.select_link_color.color }}!important;text-decoration:none;}\nbody.free a.btn {border:3px solid {{ widget_data.select_link_color.color }}!important;}\nbody.free a:hover, body.free a.btn:hover {rgba({{ widget_data.select_link_color.color|convert_rgb }}, .75);}\nbody.free .header-container-wrapper {background-color: rgba({{ widget_data.select_header_bgcolor.color|convert_rgb }},0.9);}\nbody.free .footer-container-wrapper {background-color:{{ widget_data.select_footer_bgcolor.color }};}\n/* Content Container Styles */\n.coside-on-solid {background-color:rgba({{ widget_data.container_bg_side_color.color|convert_rgb }}, 1);}\n.coside-on-transparent {background-color:rgba({{ widget_data.container_bg_side_color.color|convert_rgb }}, .6);}\n\n.hs_cos_wrapper_type_form input[type=\"text\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"password\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"datetime\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"datetime-local\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"date\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"month\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"time\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"week\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"number\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"email\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"url\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"search\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"tel\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"color\"]:focus,\n.hs_cos_wrapper_type_form input[type=\"file\"]:focus,\n.hs_cos_wrapper_type_form textarea:focus,\n.hs_cos_wrapper_type_form select:focus {\n    color:{{ widget_data.select_accent_color.color }}!important;\n    border:1px solid {{ widget_data.select_accent_color.color }}!important;\n}\n\nbody.free .btn-primary, .hs-button.primary {\n    background-color:{{ widget_data.select_accent_color.color }}!important;\n}\n\nh3.form-title {color:{{ widget_data.select_form_titlecolor.color }}!important;}\n.hs_cos_wrapper_type_form label {color:{{ widget_data.select_body_color.color }}!important;}\n.hs_cos_wrapper_type_form input[type=\"text\"],\n.hs_cos_wrapper_type_form input[type=\"password\"],\n.hs_cos_wrapper_type_form input[type=\"datetime\"],\n.hs_cos_wrapper_type_form input[type=\"datetime-local\"],\n.hs_cos_wrapper_type_form input[type=\"date\"],\n.hs_cos_wrapper_type_form input[type=\"month\"],\n.hs_cos_wrapper_type_form input[type=\"time\"],\n.hs_cos_wrapper_type_form input[type=\"week\"],\n.hs_cos_wrapper_type_form input[type=\"number\"],\n.hs_cos_wrapper_type_form input[type=\"email\"],\n.hs_cos_wrapper_type_form input[type=\"url\"],\n.hs_cos_wrapper_type_form input[type=\"search\"],\n.hs_cos_wrapper_type_form input[type=\"tel\"],\n.hs_cos_wrapper_type_form input[type=\"color\"],\n.hs_cos_wrapper_type_form input[type=\"file\"],\n.hs_cos_wrapper_type_form textarea,\n.hs_cos_wrapper_type_form select {\n    background-color:{{ widget_data.select_form_fieldbgcolor.color }}!important;\n    color:{{ widget_data.select_form_fieldcolor.color }}!important;\n}\n.btn-primary, .hs-button.primary {\n    background-color:{{ widget_data.select_accent_color.color }}!important;\n    transition: all 0.2s linear 0s;\n    -moz-transition: all 0.2s linear 0s;\n    -webkit-transition: all 0.2s linear 0s;\n    -o-transition: all 0.2s linear 0s;\n}\n\n/*------| HUBL STRUCTURE OPTIONS |------*/\n\n/* Header On or Off */\n{% if widget_data.header_option.value == 'On' %}\n.header-container-wrapper {\n    display: block!important;\n}\n{% elif widget_data.header_option.value == 'Off' %}\n.header-container-wrapper {\n    display: none!important;\n}\n{% endif %}\n</style>\n\n<!-- Custom Container Choices -->\n{% if widget_data.container_bg_side.value == 'Off' %}\n{% elif widget_data.container_bg_side.value == 'On (Solid Container)' %}\n<script>\n    $(document).ready(function($) {\n        $(\".container_option_sidebar\").addClass(\"coside-on-solid\");                \n    });\n</script>\n{% elif widget_data.container_bg_side.value == 'On (Transparent Container)' %}\n<script>\n    $(document).ready(function($) {\n        $(\".container_option_sidebar\").addClass(\"coside-on-transparent\");                \n    });\n</script>\n{% endif %}\n"
